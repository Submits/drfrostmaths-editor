{   "name": "DrFrost Private Server",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Client side DrFrostMaths editor.",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      }
  }